# {{ ansible_managed }}

allow-hotplug eth0

iface eth0 inet static
address 151.80.64.178
netmask 255.255.255.255
pointopoint 151.80.64.254
dns-nameservers 8.8.8.8
pre-up ip link set up dev eth0
post-up ip route add 185.66.194.1 via 151.80.64.254
post-up ip route add 185.66.193.1 via 151.80.64.254
post-up iptables -t mangle -A INPUT -p tcp --tcp-flags SYN,RST SYN -j TCPMSS --set-mss 1360

iface eth0 inet6 static
address 2001:41d0:c:95c::178/64
pre-up ip -6 route add 2001:41d0:c:9ff:ff:ff:ff:ff dev eth0
